{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "sourceMap": false,
    "declaration": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
  },
  "include": ["src"]
}